{
  "name": "dotsama-wallet-react",
  "description": "Highly opinionated dotsama wallet in React",
  "version": "0.0.1",
  "repository": "rmrk-team/substra-hooks",
  "author": "RMRK team",
  "homepage": "https://rmrk.app",
  "main": "dist/cjs/src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/esm/src/index.d.ts",
  "scripts": {
    "build": "yarn run build:esm && yarn run build:cjs",
    "build:esm": "tsc --module es2020 --target es2017 --outDir dist/esm",
    "build:cjs": "tsc --outDir dist/cjs"
  },
  "peerDependencies": {
    "@chakra-ui/react": "*",
    "@polkadot/api": "*",
    "@polkadot/extension-dapp": "*",
    "@polkadot/keyring": "*",
    "@polkadot/util": "*",
    "@polkadot/util-crypto": "*",
    "react": "18.*",
    "react-dom": "18.*"
  },
  "dependencies": {
    "@polkadot/react-identicon": "^2.5.1",
    "@react-icons/all-files": "^4.1.0",
    "@substra-hooks/core": "^0.0.51",
    "fuse.js": "^6.6.2",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@chakra-ui/icons": "2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@types/react": "18.0.14",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "18.0.5"
  },
  "keywords": [
    "react",
    "hooks",
    "polkadot",
    "web3"
  ]
}
